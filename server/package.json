{
  "name": "express-ts-starter-kit",
  "version": "1.0",
  "private": true,
  "scripts": {
    "dev:start": "dotenv -e .env -e .env.dev -- ts-node-dev --respawn --transpile-only src/bin/www.ts",
    "dev:build": "dotenv -e .env -e .env.dev -- tsc",
    "prod:start": "dotenv -e .env -e .env.prod -- node dist/bin/www.js",
    "prod:build": "dotenv -e .env -e .env.prod -- tsc"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^8.1.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "fs": "^0.0.1-security",
    "helmet": "^4.2.0",
    "inversify": "^7.5.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "mysql2": "^3.12.0",
    "nodemon": "^2.0.22",
    "on-finished": "^2.4.1",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.0",
    "@types/on-finished": "^2.3.5",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8"
  }
}
