    "mysql2": "^3.12.0",
    "nodemon": "^2.0.22",
    "redis": "^4.6.12",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
